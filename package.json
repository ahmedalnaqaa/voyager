    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "http-status-codes": "^1.3.0",
    "mongoose": "^5.4.2",
    "morgan": "~1.9.0",
    "twig": "~0.10.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "express-swagger-generator": "^1.1.11",
